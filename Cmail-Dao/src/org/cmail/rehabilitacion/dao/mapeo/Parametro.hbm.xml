<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    
-->
<hibernate-mapping>
  <class name="org.cmail.rehabilitacion.modelo.seguridad.Parametro" table="seg_parametro">
    
    <id name="id" type="java.lang.Long" unsaved-value="-1">
      <column name="id"/>
      <generator class="identity"/>
    </id>
    
    <property name="nombre" type="java.lang.String">
      <column name="nombre" length="45" not-null="true" unique="true"/>
    </property>
    
    <property name="descripcion" type="java.lang.String">
      <column name="descripcion" length="255" not-null="true"/>
    </property>
    
    <property name="valor" type="java.lang.String">
      <column name="valor" length="80" not-null="true"/>
    </property>
    
    <property name="tipo">
        <type name="org.cmail.rehabilitacion.dao.mapeo.util.EnumUserType">
            <param name="enumClassName">org.cmail.rehabilitacion.modelo.seguridad.TipoParametro</param>
        </type>      
    </property>
    
    <!--Auditoria-->
    <property name="auditCreateDate" type="java.util.Date">
      <column name="auditCreateDate"/>
    </property>
    
    <property name="auditUpdateDate" type="java.util.Date">
      <column name="auditUpdateDate"/>
    </property>
    
    <property name="auditCreateUser" type="java.lang.String">
      <column name="auditCreateUser"  length="45"/>
    </property>
    
    <property name="auditUpdateUser" type="java.lang.String">
      <column name="auditUpdateUser"  length="45"/>
    </property>
    <!--Fin Auditoria-->
    
  </class>
</hibernate-mapping>
