<hibernate-mapping>
  <class name="org.cmail.rehabilitacion.modelo.htp.Interpretacion" table="htp_interpretacion">
    
    <id name="id" type="java.lang.Long" unsaved-value="-1">
      <column name="id"/>
      <generator class="native"/>
    </id>
    
    <set inverse="true" name="categorias" cascade="all-delete-orphan" lazy="false">
      <key>
        <column name="id_interpretacion"/>
      </key>
      <one-to-many class="org.cmail.rehabilitacion.modelo.htp.Categoria"/>
    </set>
    
    <property name="nombre" type="java.lang.String">
      <column length="45" name="nombre" not-null="true"/>
    </property>
    
    <property name="descripcion" type="java.lang.String">
      <column length="500" name="definicion" not-null="true"/>
    </property>
    
  </class>
</hibernate-mapping>
