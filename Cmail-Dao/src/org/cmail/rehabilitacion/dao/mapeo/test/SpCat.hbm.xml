<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--    
-->
<hibernate-mapping>
    
    <class name="org.cmail.rehabilitacion.modelo.test.SpCat">

        <id name="id" type="java.lang.Long" >
            <column name="id"/>
        </id>

        <property name="raza" type="java.lang.String">
            <column length="45" name="raza"/>
        </property>                

    </class>    
  
    <sql-query name="spGetCat" callable="true" read-only="true">
        <return alias="cat" class="org.cmail.rehabilitacion.modelo.test.SpCat">
            <return-property name="id" column="id"/>
            <return-property name="raza" column="raza"/>
        </return>
        {call SpCat(?)}
    </sql-query>

</hibernate-mapping>
