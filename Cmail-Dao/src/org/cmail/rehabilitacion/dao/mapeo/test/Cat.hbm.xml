<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--    
-->
<hibernate-mapping>
  <class name="org.cmail.rehabilitacion.modelo.test.Cat" table="test_cat">
      
    <id name="id" type="java.lang.Long" >
      <column name="id"/>
      <generator class="identity"/>
    </id>
    
    <property name="raza" type="java.lang.String">
      <column length="45" name="raza" not-null="true"/>
    </property>    
        
    <joined-subclass name="org.cmail.rehabilitacion.modelo.test.DomesticCat" table="test_dom_cat">
        <key column="id"/>
        <property name="nombre" column="nombre"/>
    </joined-subclass>    
    
    
  </class>


</hibernate-mapping>
